{{#with cartItem}}
  <td>{{ name }}</td>
  <td class="price">￥{{ price }}</td>
  <td class="quantity">
    {{#if hasParent }}
      {{ quantity }}
    {{else}}
      <a href="#" {{ action "decrease" }}>-</a>
      <span> {{ quantity }} </span>
      <a href="#" {{ action "increase" }}>+</a>
    {{/if}}
  </td>
  <td class="subtotal">￥{{ subtotal }}</td>
  <td>
    {{#unless hasParent }}
      <a href="#" {{ action "deleteRecord" }}>删除</a>
    {{/unless}}
  </td>
{{/with}}
